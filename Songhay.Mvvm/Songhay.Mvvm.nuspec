<?xml version="1.0"?>
<package >
    <metadata>
        <id>Songhay.Mvvm</id>
        <version>1.0.0</version>
        <title>Songhay MVVM</title>
        <authors>bryan.wilhite@gmail.com</authors>
        <owners>rasx@songhaysystem.com</owners>
        <projectUrl>https://github.com/BryanWilhite/Songhay.Mvvm</projectUrl>
        <iconUrl>http://songhaysystem.com/Content/images/songhay_icon.png</iconUrl>
        <licenseUrl>https://raw.github.com/BryanWilhite/Songhay.Mvvm/master/LICENSE.md</licenseUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Songhay Model, View, View-Model definitions for WPF (with Prism)</description>
        <releaseNotes>This is the inital release.</releaseNotes>
        <copyright>Copyright 2017 Bryan D. Wilhite</copyright>
        <tags>MVVM, Model, View, View Model, WPF, Windows Presentation Foundation, Prism</tags>
        <dependencies>
            <dependency id="Newtonsoft.Json" version="6.0.1" />
            <dependency id="Prism" version="5.0.0" />
            <dependency id="SonghayCore" version="1.0.0" />
        </dependencies>
    </metadata>
    <files>
        <file src="bin\Release\*.dll" target="lib/net452" />
        <file src="bin\Release\*.exe" exclude="**vshost**" target="lib/net452" />
        <file src="bin\Release\*.config" target="lib/net452" />
        <file src="bin\Release\*.xml" target="lib/net452" />

        <file src="**\*.cs" exclude="obj\**" target="src" />
    </files>
</package>