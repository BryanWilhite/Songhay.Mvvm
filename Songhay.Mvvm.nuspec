<?xml version="1.0"?>
<package >
    <metadata>
        <id>Songhay.Mvvm</id>
        <version>2.2.1</version>
        <title>Songhay MVVM</title>
        <authors>bryan.wilhite@gmail.com</authors>
        <owners>rasx@songhaysystem.com</owners>
        <projectUrl>https://github.com/BryanWilhite/Songhay.Mvvm</projectUrl>
        <iconUrl>http://songhaysystem.com/Content/images/songhay_icon.png</iconUrl>
        <licenseUrl>https://raw.github.com/BryanWilhite/Songhay.Mvvm/master/LICENSE.md</licenseUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Songhay Model, View, View-Model definitions for WPF (with Prism)</description>
        <releaseNotes>Updated dependencies requirements.</releaseNotes>
        <copyright>Copyright 2017 Bryan D. Wilhite</copyright>
        <tags>MVVM, Model, View, View Model, WPF, Windows Presentation Foundation, Prism</tags>
        <dependencies>
            <group targetFramework=".NETFramework4.6.1">
                <dependency id="CommonServiceLocator" version="2.0.1" />
                <dependency id="Newtonsoft.Json" version="10.0.3" />
                <dependency id="Prism.Core" version="6.3.0" />
                <dependency id="Prism.Wpf" version="6.3.0" />
                <dependency id="SonghayCore" version="2.5.2" />
            </group>
            <group targetFramework=".NETFramework4.5.1">
                <dependency id="CommonServiceLocator" version="2.0.1" />
                <dependency id="Newtonsoft.Json" version="10.0.3" />
                <dependency id="Prism.Core" version="6.3.0" />
                <dependency id="Prism.Wpf" version="6.3.0" />
                <dependency id="SonghayCore" version="2.5.2" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="Songhay.Mvvm-net451\bin\Release\Songhay.Mvvm*.dll" target="lib\net451" />
        <file src="Songhay.Mvvm-net451\bin\Release\Songhay.Mvvm*.exe" exclude="**vshost**" target="lib\net451" />
        <file src="Songhay.Mvvm-net451\bin\Release\Songhay.Mvvm*.config" target="lib\net451" />
        <file src="Songhay.Mvvm-net451\bin\Release\Songhay.Mvvm*.xml" target="lib\net451" />

        <file src="Songhay.Mvvm\bin\Release\Songhay.Mvvm*.dll" target="lib\net461" />
        <file src="Songhay.Mvvm\bin\Release\Songhay.Mvvm*.exe" exclude="**vshost**" target="lib\net461" />
        <file src="Songhay.Mvvm\bin\Release\Songhay.Mvvm*.config" target="lib\net461" />
        <file src="Songhay.Mvvm\bin\Release\Songhay.Mvvm*.xml" target="lib\net461" />

        <file src="Songhay.Mvvm\**\*.cs" exclude="Songhay.Mvvm\**\obj\**" target="src" />
    </files>
</package>